{
  "name": "republia-times",
  "version": "0.1.0",
  "description": "A port of the flash game Republia Times to the web.",
  "author": "Brandon Johnson <bjohn465+github@gmail.com>",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "format-message": "^6.2.3",
    "format-message-generate-id": "^6.2.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:nobrowser": "cross-env BROWSER=none yarn start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watchAll": "yarn test --watchAll",
    "test:once": "yarn test --watchAll=false",
    "eject": "react-scripts eject",
    "flow": "flow",
    "prettier": "prettier",
    "eslint": "eslint .",
    "lint": "npm-run-all --parallel --continue-on-error eslint flow",
    "extract": "format-message extract -g underscored_crc32 -o src/locales/en.json 'src/**/!(*.test)*.js'",
    "cypress:open": "start-server-and-test start:nobrowser http://localhost:3000 cypress:open:final",
    "cypress:open:final": "cypress open",
    "cypress:run": "start-server-and-test start:nobrowser http://localhost:3000 cypress:run:final",
    "cypress:run:final": "cypress run"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/cypress": "^4.0.4",
    "@testing-library/react": "^8.0.4",
    "cross-env": "^5.2.0",
    "cypress": "^3.4.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-flowtype": "^3.11.1",
    "eslint-plugin-format-message": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "flow-bin": "^0.102.0",
    "format-message-cli": "^6.2.3",
    "jest-dom": "^3.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "start-server-and-test": "^1.9.1"
  }
}
