{
  "name": "republia-times",
  "version": "0.1.0",
  "description": "A port of the flash game Republia Times to the web.",
  "author": "Brandon Johnson <bjohn465+github@gmail.com>",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.22",
    "format-message": "^6.2.3",
    "format-message-generate-id": "^6.2.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "start": "cross-env EXTEND_ESLINT=true react-scripts start",
    "start:nobrowser": "cross-env BROWSER=none yarn start",
    "build": "cross-env EXTEND_ESLINT=true react-scripts build",
    "test": "react-scripts test",
    "test:watchAll": "yarn test --watchAll",
    "test:once": "yarn test --watchAll=false",
    "eject": "react-scripts eject",
    "flow": "flow",
    "prettier": "prettier",
    "eslint": "eslint .",
    "lint": "npm-run-all --parallel --continue-on-error eslint flow",
    "extract": "format-message extract -g underscored_crc32 -o src/locales/en.json 'src/**/!(*.test)*.js'",
    "cypress:open": "start-server-and-test 'yarn start:nobrowser' http://localhost:3000 'cypress open'",
    "cypress:run": "start-server-and-test 'yarn start:nobrowser' http://localhost:3000 'cypress run'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/cypress": "^5.0.2",
    "@testing-library/jest-dom": "^4.2.0",
    "@testing-library/react": "9.3.0",
    "cross-env": "^6.0.3",
    "cypress": "^3.5.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-format-message": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "flow-bin": "^0.110.1",
    "format-message-cli": "^6.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "start-server-and-test": "^1.10.6"
  }
}
